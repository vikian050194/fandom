  "scripts": {
    "lint": "eslint src --config=.eslintrc --ext=js",
    "cleanup": "rm -rf build",
    "preversion": "npm test",
    "postversion": "git push && git push --tags ",
    "prebuild": "npm run cleanup",
    "build": "webpack --config webpack.config.js",
    "prerelease": "npm run cleanup",
    "release": "webpack --config webpack.production.config.js",
    "publish-rc": "npm run release && npm version prerealese && npm publish",
    "publish-patch": "npm run release && npm version patch && npm publish",
    "publish-minor": "npm run release && npm version minor && npm publish",
    "publish-major": "npm run release && npm version major && npm publish",
    "test": "mocha ./tests/**/*-tests.js --require @babel/register",
    "cover": "nyc --reporter=text-summary --reporter=html npm run test",
    "cover:ci": "nyc --reporter=lcovonly npm run test"
  },
  "devDependencies": {
    "@babel/cli": "7.22.15",
    "@babel/core": "7.22.15",
    "@babel/eslint-parser": "7.22.15",
    "@babel/preset-env": "7.22.15",
    "@babel/register": "7.22.15",
    "assert": "2.1.0",
    "babel-loader": "9.1.3",
    "babel-plugin-istanbul": "6.1.1",
    "eslint": "8.48.0",
    "eslint-webpack-plugin": "4.0.1",
    "mocha": "10.2.0",
    "nyc": "15.1.0",
    "resolve-url-loader": "5.0.0",
    "url-loader": "4.1.1",
    "webpack": "5.88.2",
    "webpack-cli": "5.1.4",
    "webpack-merge": "5.9.0"
  }
